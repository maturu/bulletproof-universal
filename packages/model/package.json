{
  "name": "@repo/model",
  "version": "0.0.0",
  "types": "./dist",
  "main": "dist/index.mjs",
  "files": [
    "dist"
  ],
  "private": true,
  "scripts": {
    "clean": "rm -rf dist && rm -rf node_modules && rm -rf .turbo",
    "dev": "docker compose up",
    "build": "pnpm db:generate && vite build --ssr",
    "db:start": "src/scripts/db-startup.sh",
    "db:generate": "prisma generate",
    "db:push": "prisma db push --skip-generate",
    "db:migrate": "prisma migrate dev",
    "db:reset": "prisma migrate reset",
    "db:seed": "prisma db seed",
    "lint": "eslint --ext .ts,.tsx --report-unused-disable-directives --max-warnings 0",
    "types": "tsc --project tsconfig.node.json --pretty --noEmit",
    "test": "vitest -c ./vitest.config.ts run"
  },
  "prisma": {
    "seed": "tsx src/prisma/seed.ts",
    "schema": "src/prisma/schema.prisma"
  },
  "devDependencies": {
    "@faker-js/faker": "^9.8.0",
    "@quramy/prisma-fabbrica": "^2.3.0",
    "@repo/eslint-config": "workspace:*",
    "@repo/typescript-config": "workspace:*",
    "eslint": "^9.28.0",
    "tsx": "^4.20.1",
    "typescript": "5.8.2",
    "vite": "^6.3.5",
    "vite-plugin-dts": "^4.5.4",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.2.3",
    "vitest-environment-vprisma": "^1.3.0"
  },
  "dependencies": {
    "@prisma/client": "^6.9.0",
    "prisma": "^6.9.0",
    "zod": "^3.25.62",
    "zod-prisma-types": "^3.2.4"
  }
}
